							"type": "LinkedServiceReference"
						}
					},
					{
						"name": "enrichment_customer",
						"type": "DatabricksNotebook",
						"dependsOn": [
							{
								"activity": "ingest_customer",
								"dependencyConditions": [
									"Completed"
								]
							},
							{
								"activity": "ingest_customerDriver",
								"dependencyConditions": [
									"Completed"
								]
							},
							{
								"activity": "ingest_loanTransaction",
