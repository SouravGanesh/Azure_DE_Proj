{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "Azure-DE-Terraform-Proj-ADF"
		},
		"azure_de_proj_ls_accessToken": {
			"type": "secureString",
			"metadata": "Secure string for 'accessToken' of 'azure_de_proj_ls'"
