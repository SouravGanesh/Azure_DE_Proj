							"timeout": "0.12:00:00",
							"retry": 0,
							"retryIntervalInSeconds": 30,
							"secureOutput": false,
							"secureInput": false
						},
						"userProperties": [],
						"typeProperties": {
							"notebookPath": "/Users/souravganesh.crp@gmail.com/Azure_DE_Terraform_Proj/ingestion/loanTransaction",
