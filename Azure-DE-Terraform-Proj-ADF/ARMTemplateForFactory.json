							"notebookPath": "/Users/souravganesh.crp@gmail.com/Azure_DE_Terraform_Proj/ingestion/customer",
							"baseParameters": {
								"p_file_date": {
									"value": "@formatDateTime(pipeline().parameters.p_processing_date, 'yyyy-MM-dd')\n",
									"type": "Expression"
								}
							}
						},
						"linkedServiceName": {
							"referenceName": "azure_de_proj_ls",
							"type": "LinkedServiceReference"
						}
					},
					{
						"name": "ingest_customerDriver",
