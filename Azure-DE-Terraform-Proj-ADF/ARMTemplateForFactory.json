		{
			"name": "[concat(parameters('factoryName'), '/azure_de_proj_ls')]",
			"type": "Microsoft.DataFactory/factories/linkedServices",
			"apiVersion": "2018-06-01",
			"properties": {
				"annotations": [],
				"type": "AzureDatabricks",
				"typeProperties": {
					"domain": "https://adb-2262589934690181.1.azuredatabricks.net",
					"accessToken": {
						"type": "SecureString",
						"value": "[parameters('azure_de_proj_ls_accessToken')]"
